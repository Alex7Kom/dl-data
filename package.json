{
  "name": "dl-data",
  "version": "1.0.0",
  "description": "Download files in a structured way",
  "main": "src/download.js",
  "keywords": [
    "download"
  ],
  "author": "Alexey Komarov <alex7kom@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "download": "^6.2.5",
    "obj-flatten": "^2.0.3"
  },
  "devDependencies": {
    "@alex7kom/sayonara-config": "^0.5.0",
    "husky": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/@alex7kom/sayonara-config/src/configs/eslint-node.js",
    "overrides": [
      {
        "files": [
          "*.?(test|spec).{j,t}s?(x)"
        ],
        "env": {
          "jest": true,
          "node": true,
          "es6": true
        },
        "parserOptions": {
          "ecmaVersion": 2018
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{j,t}s?(x)\"",
    "pretty": "pretty-quick",
    "test": "jest"
  },
  "sayonaraConfig": {
    "sets": [
      "node-library",
      "npm-package"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
